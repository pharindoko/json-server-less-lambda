{
  "name": "json-serverless",
  "version": "0.0.0-development",
  "description": "Transform a json file into an api",
  "main": "src/server.js",
  "scripts": {
    "test": "NODE_ENV=local jest",
    "debug": "NODE_ENV=debug webpack --watch",
    "start": "NODE_ENV=local webpack --watch",
    "dev": "NODE_ENV=development webpack --watch",
    "diagnostic": "NODE_ENV=offline sls offline",
    "build": "webpack",
    "deploy": "serverless deploy --stage=dev",
    "snyk-protect": "snyk protect",
    "commit": "git-cz",
    "bootstrap": "lerna bootstrap",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run snyk-protect && npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pharindoko/json-serverless.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pharindoko/json-serverless/issues"
  },
  "homepage": "https://github.com/pharindoko/json-serverless.git#readme",
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "json-server": "^0.15.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "lowdb-adapter-aws-s3": "^1.1.2",
    "pino": "^5.10.6",
    "pino-pretty": "^3.2.0",
    "serverless-http": "^2.3.0",
    "snyk": "^1.230.5",
    "source-map-support": "^0.5.10",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@types/aws-lambda": "8.10.36",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.23",
    "@types/json-server": "0.14.2",
    "@types/lodash": "4.14.149",
    "@types/lowdb": "1.0.9",
    "@types/node": "10.14.16",
    "@types/supertest": "2.0.8",
    "@types/swagger-schema-official": "2.0.20",
    "@types/swagger-ui-express": "4.1.0",
    "copy-webpack-plugin": "5.0.5",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "express-swagger-generator": "1.1.15",
    "gts": "1.1.2",
    "jest": "24.9.0",
    "node-env-webpack-plugin": "1.1.0",
    "nodemon": "2.0.1",
    "nodemon-webpack-plugin": "4.2.1",
    "pino-pretty": "3.4.0",
    "serverless-offline": "5.12.1",
    "serverless-webpack": "5.3.1",
    "terser-webpack-plugin": "2.2.1",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "snyk": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
