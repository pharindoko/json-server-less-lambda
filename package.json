{
	"name": "json-serverless",
	"version": "0.0.0-development",
	"description": "a lambda solution for json-server",
	"main": "src/server.js",
	"scripts": {
		"test": "npm run build & NODE_ENV=local jest",
		"debug": "NODE_ENV=debug webpack --watch",
		"start": "NODE_ENV=local webpack --watch",
		"dev": "NODE_ENV=development webpack --watch",
		"diagnostic": "NODE_ENV=offline sls offline",
		"build": "babel src -d dist -D",
		"deploy": "serverless deploy --stage=dev",
		"snyk-protect": "snyk protect",
		"commit": "git-cz",
		"bootstrap": "lerna bootstrap"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pharindoko/json-serverless.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pharindoko/json-serverless/issues"
	},
	"homepage": "https://github.com/pharindoko/json-serverless.git#readme",
	"dependencies": {
		"@babel/polyfill": "7.4.4",
		"@babel/runtime": "^7.5.5",
		"babel-loader": "^8.0.6",
		"dotenv": "^8.0.0",
		"json-server": "^0.15.0",
		"lowdb": "^1.0.0",
		"lowdb-adapter-aws-s3": "^1.1.2",
		"pino": "^5.10.6",
		"pino-pretty": "^3.2.0",
		"serverless-http": "^2.0.0",
		"snyk": "^1.198.0",
		"supertest": "^4.0.2"
	},
	"devDependencies": {
		"@babel/cli": "7.5.5",
		"@babel/core": "7.5.5",
		"@babel/node": "7.5.5",
		"@babel/preset-env": "7.5.5",
		"@semantic-release/changelog": "3.0.4",
		"@semantic-release/git": "7.0.16",
		"copy-webpack-plugin": "5.0.4",
		"cz-conventional-changelog": "3.0.2",
		"eslint": "6.2.1",
		"eslint-config-airbnb-base": "14.0.0",
		"eslint-plugin-import": "2.18.2",
		"jest": "24.9.0",
		"node-env-webpack-plugin": "1.1.0",
		"nodemon": "1.19.1",
		"nodemon-webpack-plugin": "4.0.8",
		"pino-pretty": "3.2.1",
		"serverless-offline": "5.10.1",
		"serverless-webpack": "5.3.1",
		"terser-webpack-plugin": "1.4.1",
		"webpack": "4.39.2",
		"webpack-cli": "3.3.7",
		"webpack-node-externals": "1.7.2"
	},
	"snyk": true,
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
