    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-list-endpoints": "^5.0.0",
    "express-slash": "^2.0.1",
    "graphql": "^15.3.0",
    "helmet": "^4.0.0",
    "json-server": "^0.16.1",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
