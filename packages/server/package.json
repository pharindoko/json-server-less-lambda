    "serverless-http": "^2.3.2",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "swagger-to-graphql": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "table": "^5.4.6"
