    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.22.0",
    "express-swagger-generator": "1.1.17",
    "gts": "1.1.0",
    "jest": "26.3.0",
    "nodemon": "2.0.4",
